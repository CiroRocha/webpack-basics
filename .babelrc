{
  "presets": [
    "@babel/react",
    [
      "@babel/env", {
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions"
          ]
        },
        "useBuiltIns": "usage"
      }
    ]
    // "@babel/preset-stage-2",
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-react-jsx"
  ]
}