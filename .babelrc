{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions"
          ]
        },
        "useBuiltIns": "usage"
      }
    ],
    // "@babel/preset-stage-2",
    [ "@babel/preset-react" ]
  ],
  "env": {
    "development": {
      "presets": [["@babel/preset-react", { "development": true }]]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ]
}